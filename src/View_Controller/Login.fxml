<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.LoginController">
   <children>
      <Label fx:id="loginlbl" alignment="CENTER" contentDisplay="CENTER" layoutX="238.0" layoutY="42.0" text="Login" textAlignment="CENTER" textFill="#2250a1">
         <font>
            <Font name="System Bold Italic" size="48.0" />
         </font>
      </Label>
      <Label fx:id="userNamelbl" layoutX="101.0" layoutY="158.0" text="Username">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="passwordlbl" layoutX="101.0" layoutY="210.0" text="Password">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="userNametxt" layoutX="241.0" layoutY="159.0" />
      <Button fx:id="submitBtn" layoutX="212.0" layoutY="321.0" mnemonicParsing="false" onAction="#submitHandler" text="Submit" />
      <Button fx:id="closeBtn" layoutX="345.0" layoutY="321.0" mnemonicParsing="false" onAction="#closeHandler" text="Close" />
      <PasswordField fx:id="passwordPswrd" layoutX="241.0" layoutY="211.0" />
      <Label fx:id="errorlbl" alignment="CENTER" layoutX="228.0" layoutY="267.0" textFill="RED" />
   </children>
</AnchorPane>
